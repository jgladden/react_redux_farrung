{
  "name": "react_redux_farrung",
  "version": "1.0.0",
  "description": "portfolio site",
  "repository": {
    "type": "git",
    "url": "https://github.com/jgladden/react_redux_farrung.git"
  },
  "main": "src/app.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:server lint:watch",
    "dev:server": "webpack-dev-server --mode development --config webpack.base.config.js --hot --open --env.PLATFORM=local",
    "prebuild": "webpack --mode production --config webpack.prod.config.js --env.PLATFORM=production --progress",
    "build": "node server",
    "stats": "npx webpack --mode production --config webpack.prod.config.js --env.PLATFORM=production --profile --json > stats.json",
    "lint": "node_modules/.bin/esw webpack.base.config.js src --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "react"
  ],
  "jest": {
    "moduleNameMapper": {
      "^actions(.*)$": "<rootDir>/src/actions$1",
      "^reducers(.*)$": "<rootDir>/src/reducers$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^containers(.*)$": "<rootDir>/src/containers$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^config(.*)$": "<rootDir>/src/config$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ]
  },
  "author": "jgladden",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^2.1.14",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "moxios": "^0.4.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.5.7",
    "director": "^1.2.8",
    "express": "^4.16.4",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  }
}
